{
  "name": "smartspeak-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "predev": "npm run prisma:generate",
    "dev": "concurrently -n PRISMA,SERVER -c yellow,blue \"prisma generate --watch\" \"ts-node-dev --respawn --transpile-only src/index.ts\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "dependencies": {
    "@prisma/client": "^5.16.1",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.47.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.14.10",
    "axios-mock-adapter": "^1.22.0",
    "jest": "^29.7.0",
    "prisma": "^5.16.1",
    "ts-jest": "^29.2.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
